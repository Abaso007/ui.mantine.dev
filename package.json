{
  "name": "mantine-next-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 1764",
    "analyze": "ANALYZE=true next build",
    "build": "next build && echo 'ui.mantine.dev' > out/CNAME && touch out/.nojekyll",
    "deploy": "npm run build && gh-pages -d out",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint data components lib pages",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx,css}\"",
    "test": "npm run prettier:check && npm run lint && npm run typecheck",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@mantine/carousel": "8.2.5",
    "@mantine/code-highlight": "8.2.5",
    "@mantine/core": "8.2.5",
    "@mantine/dropzone": "8.2.5",
    "@mantine/form": "8.2.5",
    "@mantine/hooks": "8.2.5",
    "@mantine/spotlight": "8.2.5",
    "@mantinex/dev-icons": "^2.0.0",
    "@mantinex/mantine-header": "^2.0.0",
    "@mantinex/mantine-logo": "^2.0.0",
    "@mantinex/mantine-meta": "^2.0.0",
    "@next/bundle-analyzer": "^15.5.0",
    "@tabler/icons-react": "^3.34.1",
    "dayjs": "^1.11.13",
    "embla-carousel-react": "^8.6.0",
    "next": "15.5.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "shiki": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@eslint/js": "^9.33.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.6.2",
    "@storybook/addon-styling-webpack": "^1.0.1",
    "@storybook/blocks": "^8.6.7",
    "@storybook/nextjs": "^8.6.7",
    "@storybook/react": "^8.6.7",
    "@types/eslint-plugin-jsx-a11y": "^6",
    "@types/node": "^24.3.0",
    "@types/react": "19.1.10",
    "babel-loader": "^10.0.0",
    "eslint": "^9.33.0",
    "eslint-config-mantine": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.6.2",
    "storybook": "^8.6.7",
    "storybook-dark-mode": "^4.0.2",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.40.0"
  },
  "packageManager": "yarn@4.9.3"
}
